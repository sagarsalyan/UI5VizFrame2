<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:viz="sap.viz.ui5.controls" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:viz.data="sap.viz.ui5.data"
	xmlns:commons="sap.suite.ui.commons">
	<!--<Page>-->
	<Dialog title="Vendor Details" contentHeight="50%" contentWidth="50%" draggable="true" resizable="true">
		<content>
			<Table id="idProductsTable" inset="false" items="{ path: 'itemDetails>/'}">
				<columns>
					<!--<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">-->
					<!--	<Text text="Date"/>-->
					<!--</Column>-->
					<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
						<Text text="Vendor"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
						<Text text="Vendor Name"/>
					</Column>
					<Column minScreenWidth="Desktop" demandPopin="true" hAlign="End">
						<Text text="Plant"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<!--<Text text="{itemDetails>Date}"/>-->
							<Text text="{itemDetails>Lifnr}"/>
							<Text text="{itemDetails>Name}"/>
							<Text text="{itemDetails>Werks}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
		<endButton>
			<Button text="Close" press="onCloseTab"/>
		</endButton>
	</Dialog>
	<!--</Page>-->
</core:FragmentDefinition>